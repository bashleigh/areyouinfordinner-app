{
  "name": "areyouinfordinner2",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.1.0",
    "@types/react-native": "^0.52.21",
    "@types/react-test-renderer": "^16.0.0",
    "jest-expo": "26.0.0",
    "prettier": "^1.12.1",
    "react-native-scripts-ts": "1.13.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.0-alpha.1",
    "ts-jest": "^22.4.2",
    "tslib": "^1.9.0",
    "typescript": "^2.8.1"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "format": "prettier --write \"**/*.tsx\"",
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "expo": "^26.0.0",
    "history": "^4.7.2",
    "react": "16.3.0-alpha.1",
    "react-native": "0.54.0",
    "react-redux": "^5.0.7",
    "react-router-native": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux-form": "^7.3.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0"
  }
}
